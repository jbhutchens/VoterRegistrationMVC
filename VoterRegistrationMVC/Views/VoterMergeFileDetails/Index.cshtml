@model IEnumerable<VoterRegistrationMVC.Models.VoterMergeFileDetails>

@{
    ViewBag.Title = "Add or Edit Merge File Details";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DateMerged)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.JeffcoDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AdamsCountyDate)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DateMerged)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.JeffcoDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AdamsCountyDate)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.VoterFileID }) |
            @Html.ActionLink("Details", "Details", new { id=item.VoterFileID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.VoterFileID })
        </td>
    </tr>
}

</table>
